{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@window-splitter/vue",
  "version": "1.0.2",
  "sideEffects": false,
  "homepage": "https://react-window-splitter-six.vercel.app",
  "repository": {
    "url": "https://github.com/hipstersmoothie/window-splitter",
    "directory": "packages/vue"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "vite build",
    "test": "CI=true vitest",
    "lint": "eslint .",
    "stats": "rsbuild --mode production build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "clean": "rm -rf dist .turbo .tshy .tshy-build"
  },
  "keywords": [
    "vue",
    "panel",
    "splitter",
    "resizable",
    "window"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Andrew Lisowski",
    "email": "lisowski54@gmail.com"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT",
  "description": "A WAI-ARIA compliant window splitter for Vue.",
  "devDependencies": {
    "@internal/eslint-config": "workspace:*",
    "@storybook/vue3": "^8.6.12",
    "@storybook/vue3-vite": "^8.6.12",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/vue": "^8.1.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-istanbul": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-vue": "^10.1.0",
    "framer-motion": "^11.3.28",
    "storybook": "^8.6.12",
    "tiny-cookie": "^2.5.1",
    "ts-dedent": "^2.2.0",
    "typescript": "catalog:",
    "typescript-eslint": "^8.1.0",
    "vite": "^6.3.3",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "catalog:",
    "vue": "^3.5.13"
  },
  "dependencies": {
    "@window-splitter/interface": "workspace:*",
    "@window-splitter/state": "workspace:*"
  },
  "peerDependencies": {
    "vue": ">=3.x"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs"
}
